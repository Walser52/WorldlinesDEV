dat <- data.frame(t=seq(0, 2*pi, 0.1))
xheart <- function(t) 16*sin(t)^3
yheart <- function(t) 13*cos(t)-5*cos(2*t)-2*cos(3*t)-cos(4*t)
dat$y=yheart(dat$t)
dat$x=xheart(dat$t)
with(dat, plot(x,y, type="l"))
with(dat, polygon(x,y, col="red"))
points(c(10,-10, -15, 15), c(-10, -10, 10, 10), pch=169, font=5, col = "orange")
